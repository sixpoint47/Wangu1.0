# MongoDB connection string (replace with your real connection string when deploying)
MONGO_URL=mongodb+srv://username:password@cluster.mongodb.net/dbname?retryWrites=true&w=majority

# Port your backend will run on
PORT=5000

# JWT secret for authentication (replace with a strong secret)
JWT_SECRET=your_jwt_secret_here

# Environment (development or production)
NODE_ENV=development

# API key for a third-party service (if needed)
API_KEY=your_api_key_here
